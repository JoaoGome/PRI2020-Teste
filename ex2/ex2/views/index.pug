extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h3 Lista de Processos Nivel 1

    if (data.length > 0)
      .w3-container
        table(class="w3-table-all")
          tr
            th Titulo
            th Codigo
          each p in data
            tr
              td= p.titulo
              td
                a(href=`http://localhost:8001/classes/${p.codigo}`) #{p.codigo}
              
                
    footer(class="w3-container w3-teal")
                address Gerado por galuno :: 2020-Novembro, jcr
